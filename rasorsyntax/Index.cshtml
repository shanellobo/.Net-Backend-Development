@model List<RasorSyntax.Models.Product>

<h2>Product List</h2>
<table border="10" cellpadding="6">
    <tr>
        <th>Product ID</th>
        <th>Name</th>
        <th>Price</th>
    </tr>

    @foreach (var product in Model)
    {
        <tr>
            <td>@product.Id</td>
            <td>@product.Name</td>
            <td>@product.Price</td>
        </tr>
    }
</table>

